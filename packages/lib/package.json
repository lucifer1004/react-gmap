    "@types/googlemaps": "3.30.16",
    "@types/jest": "24.0.6",
    "@types/loadjs": "3.5.0",
    "@types/node": "11.9.5",
    "@types/react": "16.8.4",
    "@types/react-dom": "16.8.2",
    "@types/uuid": "3.4.4",
