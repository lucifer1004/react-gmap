{
  "name": "react-google-map",
  "alias": "react-google-map.gabriel-wu.com",
  "version": 2,
  "builds": [
    {"src": "build/**", "use": "@now/static"},
    {"src": "styleguide/**", "use": "@now/static"}
  ],
  "routes": [
    {
      "src": "/doc(.*)",
      "dest": "/styleguide/$1"
    },
    {
      "src": "/build/(.*)",
      "dest": "/styleguide/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ]
}
