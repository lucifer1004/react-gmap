{
  "name": "react-google-map",
  "private": true,
  "description": "Google Map component for React.",
  "repository": "git@github.com:lucifer1004/react-google-map.git",
  "author": "Gabriel Wu <qqbbnease1004@126.com>",
  "license": "MIT",
  "dependencies": {
    "dotenv": "6.2.0",
    "loadjs": "3.5.5",
    "react": "16.8.2",
    "react-dom": "16.8.2",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "husky": "1.3.1",
    "lerna": "3.13.0",
    "lint-staged": "8.1.4",
    "prettier": "1.16.4",
    "react-scripts": "2.1.5"
  },
  "scripts": {
    "start:CRA": "cd examples/CRA && yarn start",
    "build": "cd examples/CRA && yarn build",
    "publish:lib": "cd packages/lib && yarn publish",
    "test:lib": "cd packages/lib && yarn test",
    "styleguide": "cd packages/lib && yarn styleguide",
    "styleguide:build": "cd packages/lib && yarn styleguide:build",
    "deploy:now": "sh deploy.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test --coverage && lint-staged"
    }
  },
  "workspaces": [
    "examples/*",
    "packages/*"
  ]
}
