{
  "name": "@lucifer1004/react-google-map",
  "version": "0.2.0",
  "description": "Google Map component for React.",
  "repository": "git@github.com:lucifer1004/react-gmap.git",
  "author": "Gabriel Wu <qqbbnease1004@126.com>",
  "license": "MIT",
  "source": "index.ts",
  "main": "dist/index.js",
  "dependencies": {
    "loadjs": "3.5.5",
    "react": "16.8.0-alpha.1",
    "react-dom": "16.8.0-alpha.1"
  },
  "scripts": {
    "build": "cp ../../*.md . && rm -rf dist && microbundle && rm -rf .rts2_cache_*",
    "prepublish": "yarn build",
    "postpublish": "rm -rf dist"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn test --coverage && lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!node_modules/",
      "!src/index.tsx",
      "!src/stories/**"
    ]
  }
}
